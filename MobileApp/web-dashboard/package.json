    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.2",
    "typescript": "~5.6.0",
    "web-vitals": "^2.1.4",
    "zod": "^4.3.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
